<xsd:schema targetNamespace="omeasure" xmlns:omeasure="omeasure"
	      xmlns="omeasure" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
              elementFormDefault="qualified" version="3.1.1.2">
	<xsd:annotation>
		<xsd:documentation>Заходи щодо скорочення викидів забруднюючих речовин
				  за версією обгрунтуючих матеріалів. Базові типи</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="measureClass">
		<xsd:annotation>
			<xsd:documentation>Тип класс природоохоронного заходу</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="measureClass" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Класс заходу</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="measure">
		<xsd:annotation>
			<xsd:documentation>Тип природоохоронний захід</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
                  <xsd:extension base="measureClass">
		   <xsd:sequence>
			<xsd:element name="measureName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Найменування заходу</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		  </xsd:sequence>
                 </xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="measureS">
		<xsd:annotation>
			<xsd:documentation>Класи природоохоронних заходи</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="measure" type="measureClass" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Тип природоохоронний захід</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>