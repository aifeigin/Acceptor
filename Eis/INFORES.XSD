<xsd:schema targetNamespace="infores" xmlns:infores="infores" xmlns="infores" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" version="3.1.1.2.">
    <xsd:annotation>
        <xsd:documentation>Джерело інформації</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType name="infoRef">
        <xsd:annotation>
            <xsd:documentation>Посилка на джерело інформації</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="infoRefID" type="xsd:positiveInteger">
                <xsd:annotation>
                    <xsd:documentation>Номер джерела інформації</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="infoBookmark" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Посилка на текст джерела інформації</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="infoRes">
        <xsd:annotation>
            <xsd:documentation>Джерело інформації</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="infoRefID" type="xsd:positiveInteger">
                <xsd:annotation>
                    <xsd:documentation>Номер джерела інформації</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="infoTitle" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Найменування джерела інформації</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>