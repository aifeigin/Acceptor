<xsd:schema targetNamespace="equip" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="equip" xmlns:equip="equip" elementFormDefault="qualified" version="3.1.1.2" xml:lang="uk">
	<xsd:annotation>
		<xsd:documentation>Додаток 2. Перелiк виробничих та технологiчних процесiв,  технологiчного устаткування (установок).
        </xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="equipmenT">
		<xsd:annotation>
			<xsd:documentation>Технологічне устаткування</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="equipmenTCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Код устаткування</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="equipmenTName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Найменування устаткування</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="technologY">
		<xsd:annotation>
			<xsd:documentation>Технологічні процеси</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="technologYCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Код процесу</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="technologYName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Найменування процесу</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="equipmenT" type="equipmenT" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Технологічне устаткування</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="industrY">
		<xsd:annotation>
			<xsd:documentation>Виробнича галузь</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="industrYCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Код галузі</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="industrYName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Найменування галузі</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="technologY" type="technologY" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Технологічні процеси</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="industrYProcess">
		<xsd:annotation>
			<xsd:documentation>Виробничі процеси</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="industrY" type="industrY" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Виробничі налузі</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>