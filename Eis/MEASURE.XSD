<xsd:schema targetNamespace="measure" xmlns:measure="measure" xmlns="measure" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" version="3.1.1.2">
	<xsd:annotation>
		<xsd:documentation>Додаток 3. Заходи. Базові типи</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="measure">
		<xsd:annotation>
			<xsd:documentation> Тип природоохоронний захід</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="measureCode" type="xsd:integer" nillable="true">
				<xsd:annotation>
					<xsd:documentation>Код заходу</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="measureName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Найменування заходу</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="measureS">
		<xsd:annotation>
			<xsd:documentation> Тип природоохороннi заходи</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="measure" type="measure" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Тип природоохоронний захід</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>