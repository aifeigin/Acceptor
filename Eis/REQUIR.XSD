<xsd:schema targetNamespace="req" xmlns:req="req" xmlns:emisrang="emisrang" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="req" elementFormDefault="qualified" version="3.1.1.2" xml:lang="uk">
	<xsd:annotation>
		<xsd:documentation>Умови</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="conditioN">
		<xsd:annotation>
			<xsd:documentation>Умови</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="titlE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Найменування умови</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="requiremenT" minOccurs="1" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Вимоги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="ID"  type="xsd:string"/>		
	</xsd:complexType>

	<xsd:element name="requiremenT">
		<xsd:complexType>
			<xsd:annotation>
				<xsd:documentation>Вимоги</xsd:documentation>
			</xsd:annotation>
			<xsd:sequence>
				<xsd:element name="titlE" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Найменування вимоги</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="casE" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>випадки застосування</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="ID" type="xsd:string"/>
			<xsd:attribute name="REF" type="xsd:string"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="casE">
            <xsd:complexType>
		<xsd:annotation>
			<xsd:documentation>Випадки застосування</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
		        <xsd:element name="titlE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Найменування випадка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="requiremenT" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Вимоги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
