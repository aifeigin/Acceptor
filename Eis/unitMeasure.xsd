<xsd:schema targetNamespace="unitmeasure" xmlns:unitmeasure="unitmeasure" xmlns="unitmeasure" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" version="3.1.1.2">
	<xsd:annotation>
		<xsd:documentation>Одиниці виміру згідно УКТ ЗЕД</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="unitMeasureCode">
		<xsd:annotation>
			<xsd:documentation>Код одиниці виміру</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
 		 <xsd:element name="digiT" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>цифрове позначення одиниці виміру</xsd:documentation>
			</xsd:annotation>
		 </xsd:element>
		 <xsd:element name="literaL" type="xsd:string" nillable="true">
			<xsd:annotation>
				<xsd:documentation>літерне позначення одиниці виміру</xsd:documentation>
			</xsd:annotation>
		 </xsd:element>
	             </xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="unitMeasure">
		<xsd:annotation>
			<xsd:documentation>Тип одиниця виміру</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="codE" type="unitmeasure:unitMeasureCode">
				<xsd:annotation>
					<xsd:documentation>Код одиниці виміру</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="longName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Найменування одиниці вимірювання</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="shortName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Умовне позначення</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="unitMeasureS">
		<xsd:annotation>
			<xsd:documentation>Тип одиниці виміру</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="unitMeasure" type="unitmeasure:unitMeasure" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Тип одиниці виміру</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>